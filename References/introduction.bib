@article{williamsGrowingImpactWildfire2022,
  title = {Growing Impact of Wildfire on Western {{US}} Water Supply},
  author = {Williams, A. Park and Livneh, Ben and McKinnon, Karen A. and Hansen, Winslow D. and Mankin, Justin S. and Cook, Benjamin I. and Smerdon, Jason E. and {Varuolo-Clarke}, Arianna M. and Bjarke, Nels R. and Juang, Caroline S. and Lettenmaier, Dennis P.},
