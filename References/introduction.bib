@article{sword-danielsEmbodiedUncertaintyLiving2018,
  title = {Embodied Uncertainty: Living with Complexity and Natural Hazards},
  shorttitle = {Embodied Uncertainty},
  author = {{Sword-Daniels}, Victoria and Eriksen, Christine and {Hudson-Doyle}, Emma E. and Alaniz, Ryan and Adler, Carolina and Schenk, Todd and Vallance, Suzanne},
