  shorttitle = {Community-Based {{Participatory Research}} ({{CBPR}})},
  author = {Collins, Susan E. and Clifasefi, Seema L. and Stanton, Joey and Straits, Kee J. E. and Espinosa, Patricia Rodriguez and Andrasik, Michele P. and Miller, Kimberly A. and Orfaly, Victoria E. and {Gil-Kashiwabara}, Eleanor and Nicasio, Andel V. and Hawes, Starlyn M. and Nelson, Lonnie A. and Duran, Bonnie M. and Wallerstein, Nina},
