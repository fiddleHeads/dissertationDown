@article{nelsonWildfiredependentChangesSoil2022,
  title = {Wildfire-Dependent Changes in Soil Microbiome Diversity and Function},
  author = {Nelson, Amelia R. and Narrowe, Adrienne B. and Rhoades, Charles C. and Fegel, Timothy S. and Daly, Rebecca A. and Roth, Holly K. and Chu, Rosalie K. and Amundson, Kaela K. and Young, Robert B. and Steindorff, Andrei S. and Mondo, Stephen J. and Grigoriev, Igor V. and Salamov, Asaf and Borch, Thomas and Wilkins, Michael J.},
