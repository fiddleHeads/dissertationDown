@article{calkinNegativeConsequencesPositive2015,
  title = {Negative Consequences of Positive Feedbacks in {{US}} Wildfire Management},
  author = {Calkin, David E and Thompson, Matthew P and Finney, Mark A},
  year = {2015},
  month = dec,
  journal = {Forest Ecosystems},
  volume = {2},
