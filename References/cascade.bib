@article{barnardWildfireClimateChange2023,
  title = {Wildfire and Climate Change Amplify Knowledge Gaps Linking Mountain Source-Water Systems and Agricultural Water Supply in the Western {{United States}}},
  author = {Barnard, David M. and Green, Timothy R. and Mankin, Kyle R. and DeJonge, Kendall C. and Rhoades, Charles C. and Kampf, Stephanie K. and Giovando, Jeremy and Wilkins, Mike J. and Mahood, Adam L. and Sears, Megan G. and Comas, Louise H. and Gleason, Sean M. and Zhang, Huihui and Fassnacht, Steven R. and Harmel, R. Daren and Altenhofen, Jon},
  year = {2023},
