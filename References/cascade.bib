  file = {C:\Users\mdaur\Documents\MEGAsync\Zotero\Nelson et al_2022_Wildfire-dependent changes in soil microbiome diversity and function.pdf}
}

@article{nicoleRiskPerceptionsMitigation2021,
  title = {Risk Perceptions and Mitigation Behaviors of Residents Following a Near-Miss Wildfire},
  author = {Nicole, Lauren and Larsen, Dupey and Howe, Peter D and Brunson, Mark and Yocom, Larissa and Mcavoy, Darren and Berry, E Helen and Smith, Jordan W},
